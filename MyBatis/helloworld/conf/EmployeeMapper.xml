<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.testcy.mapper.EmployeeMapper">
    <select id="getEmpById" resultType="com.testcy.bean.Employee">
        select * from `t_employee` where id = #{id}
